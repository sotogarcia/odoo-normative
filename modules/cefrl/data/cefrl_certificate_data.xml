    <field name="group_id" ref="cefrl.cefrl_certificate_group_celga"/>
    <field name="active" eval="True"/>
    <field name="description"><![CDATA[CEFR B2. Can understand the main ideas
of complex texts on both concrete and abstract topics. Can interact with a
degree of fluency and spontaneity and produce clear, detailed text, including
on technical subjects.]]></field>
  </record>

  <record id="cefrl_certificate_celga_4" model="cefrl.certificate">
    <field name="code">CELGA 4</field>
    <field name="name">CELGA 4</field>
