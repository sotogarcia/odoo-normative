    <field name="group_id" ref="cefrl.cefrl_certificate_group_celga"/>
    <field name="active" eval="True"/>
    <field name="description"><![CDATA[CEFR C1. Can understand a wide range of
 demanding, longer texts and recognize implicit meaning. Can express ideas
 fluently and spontaneously and use the language flexibly and effectively for
 social, academic and professional purposes.]]></field>
  </record>

  <record id="cefrl_certificate_celga_5" model="cefrl.certificate">
    <field name="code">CELGA 5</field>
