<odoo noupdate="0">

  <record id="cefrl_certificate_celga_1" model="cefrl.certificate">
    <field name="code">CELGA 1</field>
    <field name="name">CELGA 1</field>
    <field name="level_id" ref="cefrl.cefrl_level_A2"/>
    <field name="language_id" ref="cefrl.cefrl_language_gl"/>
    <field name="group_id" ref="cefrl.cefrl_certificate_group_celga"/>
    <field name="active" eval="True"/>
    <field name="description"><![CDATA[CEFR A2. Can understand and use
everyday expressions and basic phrases. Can communicate in simple, routine
tasks requiring a direct exchange of information.]]></field>
  </record>

  <record id="cefrl_certificate_celga_2" model="cefrl.certificate">
    <field name="code">CELGA 2</field>
    <field name="name">CELGA 2</field>
    <field name="level_id" ref="cefrl.cefrl_level_B1"/>
    <field name="language_id" ref="cefrl.cefrl_language_gl"/>
    <field name="group_id" ref="cefrl.cefrl_certificate_group_celga"/>
    <field name="active" eval="True"/>
    <field name="description"><![CDATA[CEFR B1. Can deal with most situations
likely to arise while travelling. Can produce simple connected text on
familiar topics and describe experiences, events, and briefly give reasons
and explanations.]]></field>
  </record>

  <record id="cefrl_certificate_celga_3" model="cefrl.certificate">
    <field name="code">CELGA 3</field>
    <field name="name">CELGA 3</field>
